#include <stdio.h>
#include <stdlib.h>

int main(void) {


	char string[25];
	string[0]='M';
	string[1]='A';
	string[2]='U';
	string[3]='R';
	string[4]='Y';




	//puts("!!!Hello World!!!"); /* prints !!!Hello World!!! */
	printf("%c",string[0]);

	return EXIT_SUCCESS;



}
